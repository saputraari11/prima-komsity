    <title>Telyu Place</title>
    <link rel="stylesheet" href="css/index/layout.css">
    <link rel="stylesheet" href="css/place/style.css">
</head>

<body>
    <header class="container" style="background-image: url(img/place/banner.png);" id="page-detail">
        <!-- Navbar -->
        <nav>
            <a href="#" class="logo">
                <img src="img/index/logo.png" alt="">
