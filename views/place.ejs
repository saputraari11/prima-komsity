        <div class="c-box">
            <% places.forEach((place) => {%>
            <div class="box">
                    <div class="title"><%= place.name%> (<%= place.nickname%>)</div>
                    <p><%= place.description %></p>
                    <a href="<%= place.urlDetail %>" class="c-primary">View More</a>
            </div>
            <% }) %>
           
            
        </div>
    </section>

    <!-- footer -->
    <footer class="container c-primary">
        <div class="f-left">
            <img src="img/index/logo-footer.png" alt="" class="logo-footer">
